  <bpmn:error id="Error_Critical" name="CriticalError" errorCode="ERR_CRITICAL" />
  <bpmn:message id="Message_Cancel" name="CancellationMessage" />
  <bpmn:process id="Process_EventSubprocess" name="Event Subprocess - Error and Timer Handlers" isExecutable="true" camunda:historyTimeToLive="P180D">
    <bpmn:startEvent id="Start" name="Process Started">
      <bpmn:outgoing>Flow1</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:userTask id="EnterData" name="Enter Data" camunda:assignee="${initiator}">
      <bpmn:incoming>Flow1</bpmn:incoming>
      <bpmn:outgoing>Flow2</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:serviceTask id="ProcessData" name="Process Data" camunda:asyncBefore="true" camunda:type="external" camunda:topic="data-process">
      <bpmn:incoming>Flow2</bpmn:incoming>
      <bpmn:outgoing>Flow3</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:serviceTask id="SaveResult" name="Save Result" camunda:class="com.example.ResultSaver">
      <bpmn:incoming>Flow3</bpmn:incoming>
      <bpmn:outgoing>Flow4</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:endEvent id="End" name="Completed">
      <bpmn:incoming>Flow4</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:subProcess id="ErrorHandler" name="Error Handler" triggeredByEvent="true">
      <bpmn:startEvent id="ErrorStart" name="Critical Error">
        <bpmn:outgoing>EFlow1</bpmn:outgoing>
        <bpmn:errorEventDefinition id="ED1" errorRef="Error_Critical" />
      </bpmn:startEvent>
      <bpmn:userTask id="ReviewError" name="Review Error" camunda:candidateGroups="support">
        <bpmn:incoming>EFlow1</bpmn:incoming>
        <bpmn:outgoing>EFlow2</bpmn:outgoing>
      </bpmn:userTask>
      <bpmn:sendTask id="NotifyAdmin" name="Notify Admin" camunda:delegateExpression="${adminNotifier}">
        <bpmn:incoming>EFlow2</bpmn:incoming>
        <bpmn:outgoing>EFlow3</bpmn:outgoing>
      </bpmn:sendTask>
      <bpmn:endEvent id="ErrorEnd" name="Error Handled">
        <bpmn:incoming>EFlow3</bpmn:incoming>
      </bpmn:endEvent>
      <bpmn:sequenceFlow id="EFlow1" sourceRef="ErrorStart" targetRef="ReviewError" />
      <bpmn:sequenceFlow id="EFlow2" sourceRef="ReviewError" targetRef="NotifyAdmin" />
      <bpmn:sequenceFlow id="EFlow3" sourceRef="NotifyAdmin" targetRef="ErrorEnd" />
    </bpmn:subProcess>
    <bpmn:subProcess id="TimerHandler" name="Progress Monitor" triggeredByEvent="true">
      <bpmn:startEvent id="TimerStart" name="Every Hour" isInterrupting="false">
        <bpmn:outgoing>TFlow1</bpmn:outgoing>
        <bpmn:timerEventDefinition id="TD1">
          <bpmn:timeCycle>R/PT1H</bpmn:timeCycle>
        </bpmn:timerEventDefinition>
      </bpmn:startEvent>
      <bpmn:serviceTask id="CheckProgress" name="Check Progress" camunda:type="external" camunda:topic="progress-check">
        <bpmn:incoming>TFlow1</bpmn:incoming>
        <bpmn:outgoing>TFlow2</bpmn:outgoing>
      </bpmn:serviceTask>
      <bpmn:endEvent id="TimerEnd" name="Check Done">
        <bpmn:incoming>TFlow2</bpmn:incoming>
      </bpmn:endEvent>
      <bpmn:sequenceFlow id="TFlow1" sourceRef="TimerStart" targetRef="CheckProgress" />
      <bpmn:sequenceFlow id="TFlow2" sourceRef="CheckProgress" targetRef="TimerEnd" />
    </bpmn:subProcess>
    <bpmn:subProcess id="CancelHandler" name="Cancel Handler" triggeredByEvent="true">
      <bpmn:startEvent id="CancelStart" name="Cancellation Received">
        <bpmn:outgoing>CFlow1</bpmn:outgoing>
        <bpmn:messageEventDefinition id="MD1" messageRef="Message_Cancel" />
      </bpmn:startEvent>
      <bpmn:serviceTask id="Rollback" name="Rollback Changes" camunda:class="com.example.Rollback">
        <bpmn:incoming>CFlow1</bpmn:incoming>
        <bpmn:outgoing>CFlow2</bpmn:outgoing>
      </bpmn:serviceTask>
      <bpmn:endEvent id="CancelEnd" name="Cancelled">
        <bpmn:incoming>CFlow2</bpmn:incoming>
      </bpmn:endEvent>
      <bpmn:sequenceFlow id="CFlow1" sourceRef="CancelStart" targetRef="Rollback" />
      <bpmn:sequenceFlow id="CFlow2" sourceRef="Rollback" targetRef="CancelEnd" />
    </bpmn:subProcess>
    <bpmn:sequenceFlow id="Flow1" sourceRef="Start" targetRef="EnterData" />
    <bpmn:sequenceFlow id="Flow2" sourceRef="EnterData" targetRef="ProcessData" />
    <bpmn:sequenceFlow id="Flow3" sourceRef="ProcessData" targetRef="SaveResult" />
    <bpmn:sequenceFlow id="Flow4" sourceRef="SaveResult" targetRef="End" />
  </bpmn:process>
