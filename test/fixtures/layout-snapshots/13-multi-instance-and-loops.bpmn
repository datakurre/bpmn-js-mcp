<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" xmlns:di="http://www.omg.org/spec/DD/20100524/DI" xmlns:camunda="http://camunda.org/schema/1.0/bpmn" id="Definitions_1" targetNamespace="http://bpmn.io/schema/bpmn">
  <bpmn:process id="Process_MultiInstance" name="Multi-Instance and Loops" isExecutable="true" camunda:historyTimeToLive="P180D">
    <bpmn:startEvent id="Start" name="Items Received">
      <bpmn:outgoing>Flow1</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:serviceTask id="ParallelMI" name="Process Each Item" camunda:type="external" camunda:topic="item-process">
      <bpmn:incoming>Flow1</bpmn:incoming>
      <bpmn:outgoing>Flow2</bpmn:outgoing>
      <bpmn:multiInstanceLoopCharacteristics camunda:collection="${items}" camunda:elementVariable="item">
        <bpmn:completionCondition>${nrOfCompletedInstances &gt;= nrOfInstances}</bpmn:completionCondition>
      </bpmn:multiInstanceLoopCharacteristics>
    </bpmn:serviceTask>
    <bpmn:userTask id="SequentialMI" name="Review Each Item" camunda:candidateGroups="reviewers">
      <bpmn:incoming>Flow2</bpmn:incoming>
      <bpmn:outgoing>Flow3</bpmn:outgoing>
      <bpmn:multiInstanceLoopCharacteristics isSequential="true" camunda:collection="${items}" camunda:elementVariable="item">
        <bpmn:loopCardinality>${items.size()}</bpmn:loopCardinality>
      </bpmn:multiInstanceLoopCharacteristics>
    </bpmn:userTask>
    <bpmn:serviceTask id="StandardLoop" name="Retry Upload" camunda:class="com.example.Uploader">
      <bpmn:incoming>Flow3</bpmn:incoming>
      <bpmn:outgoing>Flow4</bpmn:outgoing>
      <bpmn:standardLoopCharacteristics testBefore="true">
        <bpmn:loopCondition>${retryCount &lt; 3 and not uploadSuccess}</bpmn:loopCondition>
      </bpmn:standardLoopCharacteristics>
    </bpmn:serviceTask>
    <bpmn:subProcess id="ParallelMISub" name="Process Batch">
      <bpmn:incoming>Flow4</bpmn:incoming>
      <bpmn:outgoing>Flow5</bpmn:outgoing>
      <bpmn:multiInstanceLoopCharacteristics camunda:collection="${batches}" camunda:elementVariable="batch" />
      <bpmn:startEvent id="SubStart">
        <bpmn:outgoing>SF1</bpmn:outgoing>
      </bpmn:startEvent>
      <bpmn:serviceTask id="ProcessBatch" name="Process Batch" camunda:type="external" camunda:topic="batch-process">
        <bpmn:incoming>SF1</bpmn:incoming>
        <bpmn:outgoing>SF2</bpmn:outgoing>
      </bpmn:serviceTask>
      <bpmn:endEvent id="SubEnd">
        <bpmn:incoming>SF2</bpmn:incoming>
      </bpmn:endEvent>
      <bpmn:sequenceFlow id="SF1" sourceRef="SubStart" targetRef="ProcessBatch" />
      <bpmn:sequenceFlow id="SF2" sourceRef="ProcessBatch" targetRef="SubEnd" />
    </bpmn:subProcess>
    <bpmn:endEvent id="End" name="All Processed">
      <bpmn:incoming>Flow5</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:sequenceFlow id="Flow1" sourceRef="Start" targetRef="ParallelMI" />
    <bpmn:sequenceFlow id="Flow2" sourceRef="ParallelMI" targetRef="SequentialMI" />
    <bpmn:sequenceFlow id="Flow3" sourceRef="SequentialMI" targetRef="StandardLoop" />
    <bpmn:sequenceFlow id="Flow4" sourceRef="StandardLoop" targetRef="ParallelMISub" />
    <bpmn:sequenceFlow id="Flow5" sourceRef="ParallelMISub" targetRef="End" />
  </bpmn:process>
  <bpmndi:BPMNDiagram id="BPMNDiagram_Process_MultiInstance">
    <bpmndi:BPMNPlane id="BPMNPlane_Process_MultiInstance" bpmnElement="Process_MultiInstance">
      <bpmndi:BPMNShape id="Start_di" bpmnElement="Start">
        <dc:Bounds x="192" y="114" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="172" y="90" width="76" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="ParallelMI_di" bpmnElement="ParallelMI">
        <dc:Bounds x="288" y="92" width="100" height="80" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="SequentialMI_di" bpmnElement="SequentialMI">
        <dc:Bounds x="448" y="92" width="100" height="80" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="StandardLoop_di" bpmnElement="StandardLoop">
        <dc:Bounds x="608" y="92" width="100" height="80" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="ParallelMISub_di" bpmnElement="ParallelMISub">
        <dc:Bounds x="768" y="92" width="100" height="80" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="End_di" bpmnElement="End">
        <dc:Bounds x="928" y="114" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="912" y="160" width="68" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNEdge id="Flow1_di" bpmnElement="Flow1">
        <di:waypoint x="228" y="132" />
        <di:waypoint x="288" y="132" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow2_di" bpmnElement="Flow2">
        <di:waypoint x="388" y="132" />
        <di:waypoint x="448" y="132" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow3_di" bpmnElement="Flow3">
        <di:waypoint x="548" y="132" />
        <di:waypoint x="608" y="132" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow4_di" bpmnElement="Flow4">
        <di:waypoint x="708" y="132" />
        <di:waypoint x="768" y="132" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow5_di" bpmnElement="Flow5">
        <di:waypoint x="868" y="132" />
        <di:waypoint x="928" y="132" />
      </bpmndi:BPMNEdge>
    </bpmndi:BPMNPlane>
  </bpmndi:BPMNDiagram>
  <bpmndi:BPMNDiagram>
    <bpmndi:BPMNPlane bpmnElement="ParallelMISub">
      <bpmndi:BPMNShape id="SubStart_di" bpmnElement="SubStart">
        <dc:Bounds x="180" y="182" width="36" height="36" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="ProcessBatch_di" bpmnElement="ProcessBatch">
        <dc:Bounds x="298" y="160" width="100" height="80" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="SubEnd_di" bpmnElement="SubEnd">
        <dc:Bounds x="480" y="182" width="36" height="36" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNEdge id="SF1_di" bpmnElement="SF1">
        <di:waypoint x="216" y="200" />
        <di:waypoint x="298" y="200" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="SF2_di" bpmnElement="SF2">
        <di:waypoint x="398" y="200" />
        <di:waypoint x="480" y="200" />
      </bpmndi:BPMNEdge>
    </bpmndi:BPMNPlane>
  </bpmndi:BPMNDiagram>
</bpmn:definitions>
