      <bpmn:incoming>Flow_RequestClarification_to_WaitForResponse</bpmn:incoming>
      <bpmn:outgoing>Flow_WaitForResponse_to_ReviewResolution</bpmn:outgoing>
      <bpmn:messageEventDefinition messageRef="Msg_SupportResponse" />
    </bpmn:intermediateCatchEvent>
    <bpmn:userTask id="UserTask_ReviewResolution" name="Review Resolution">
      <bpmn:incoming>Flow_WaitForResponse_to_ReviewResolution</bpmn:incoming>
      <bpmn:outgoing>Flow_ReviewResolution_to_Resolved</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:exclusiveGateway id="Gateway_Resolved" name="Resolved?">
