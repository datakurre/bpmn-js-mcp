    <bpmn:sequenceFlow id="Flow_WaitForResponse_to_ReviewResolution" sourceRef="Event_WaitForResponse" targetRef="UserTask_ReviewResolution" />
    <bpmn:sequenceFlow id="Flow_ReviewResolution_to_Resolved" sourceRef="UserTask_ReviewResolution" targetRef="Gateway_Resolved" />
    <bpmn:endEvent id="EndEvent_Done" name="Done">
      <bpmn:incoming>Flow_Yes</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:sendTask id="SendTask_RequestClarification" name="Request Clarification">
      <bpmn:incoming>Flow_No</bpmn:incoming>
      <bpmn:outgoing>Flow_RequestClarification_to_WaitForResponse</bpmn:outgoing>
    </bpmn:sendTask>
    <bpmn:sequenceFlow id="Flow_Yes" name="Yes" sourceRef="Gateway_Resolved" targetRef="EndEvent_Done">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${resolved == true}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_No" name="No" sourceRef="Gateway_Resolved" targetRef="SendTask_RequestClarification">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${resolved == false}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_RequestClarification_to_WaitForResponse" sourceRef="SendTask_RequestClarification" targetRef="Event_WaitForResponse" />
