      <bpmn:outgoing>Flow_Yes</bpmn:outgoing>
      <bpmn:outgoing>Flow_No</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:sequenceFlow id="Flow_IssueReported_to_SubmitTicket" sourceRef="StartEvent_IssueReported" targetRef="SendTask_SubmitTicket" />
