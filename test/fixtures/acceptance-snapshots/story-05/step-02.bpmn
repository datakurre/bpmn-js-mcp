      <bpmn:outgoing>Flow_Join_to_FinalReview</bpmn:outgoing>
    </bpmn:parallelGateway>
    <bpmn:sequenceFlow id="Flow_CreditCheck_to_Join" sourceRef="ServiceTask_CreditCheck" targetRef="Gateway_Join" />
    <bpmn:sequenceFlow id="Flow_BackgroundCheck_to_Join" sourceRef="CallActivity_BackgroundCheck" targetRef="Gateway_Join" />
    <bpmn:userTask id="UserTask_FinalReview" name="Final Review">
