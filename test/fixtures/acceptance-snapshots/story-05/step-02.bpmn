    <bpmn:sequenceFlow id="Flow_Fork_to_BackgroundCheck" sourceRef="Gateway_Fork" targetRef="CallActivity_BackgroundCheck" />
    <bpmn:parallelGateway id="Gateway_Join" name="Join">
