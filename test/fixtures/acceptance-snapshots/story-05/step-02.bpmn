    </bpmn:userTask>
    <bpmn:sequenceFlow id="Flow_Join_to_FinalReview" sourceRef="Gateway_Join" targetRef="UserTask_FinalReview" />
    <bpmn:endEvent id="EndEvent_OnboardingComplete" name="Onboarding Complete">
