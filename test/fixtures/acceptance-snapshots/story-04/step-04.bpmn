      </bpmn:endEvent>
      <bpmn:sequenceFlow id="Flow_SendReceipt_to_SubEnd" sourceRef="ServiceTask_SendReceipt" targetRef="EndEvent_SubEnd" />
      <bpmn:boundaryEvent id="Event_hhmdeuz" cancelActivity="false" attachedToRef="ServiceTask_ChargeCard">
