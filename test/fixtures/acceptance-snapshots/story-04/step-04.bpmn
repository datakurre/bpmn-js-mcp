      </bpmn:serviceTask>
      <bpmn:sequenceFlow id="Flow_SubStart_to_ChargeCard" sourceRef="StartEvent_SubStart" targetRef="ServiceTask_ChargeCard" />
      <bpmn:serviceTask id="ServiceTask_SendReceipt" name="Send Receipt">
        <bpmn:incoming>Flow_ChargeCard_to_SendReceipt</bpmn:incoming>
        <bpmn:outgoing>Flow_SendReceipt_to_SubEnd</bpmn:outgoing>
      </bpmn:serviceTask>
      <bpmn:sequenceFlow id="Flow_ChargeCard_to_SendReceipt" sourceRef="ServiceTask_ChargeCard" targetRef="ServiceTask_SendReceipt" />
      <bpmn:endEvent id="EndEvent_SubEnd" name="Sub End">
