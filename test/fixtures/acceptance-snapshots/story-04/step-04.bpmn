          <bpmn:timeDuration xsi:type="bpmn:tFormalExpression">PT30S</bpmn:timeDuration>
        </bpmn:timerEventDefinition>
      </bpmn:boundaryEvent>
    </bpmn:subProcess>
    <bpmn:sequenceFlow id="Flow_Start_to_ProcessPayment" sourceRef="StartEvent_Start" targetRef="SubProcess_ProcessPayment" />
    <bpmn:endEvent id="EndEvent_PaymentDone" name="Payment Done">
      <bpmn:incoming>Flow_ProcessPayment_to_PaymentDone</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:sequenceFlow id="Flow_ProcessPayment_to_PaymentDone" sourceRef="SubProcess_ProcessPayment" targetRef="EndEvent_PaymentDone" />
    <bpmn:boundaryEvent id="Event_phu49rm" attachedToRef="SubProcess_ProcessPayment">
      <bpmn:outgoing>Flow_c453ctb</bpmn:outgoing>
      <bpmn:errorEventDefinition errorRef="Error_PaymentFailed" />
    </bpmn:boundaryEvent>
    <bpmn:userTask id="UserTask_HandlePaymentFailure" name="Handle Payment Failure">
      <bpmn:incoming>Flow_c453ctb</bpmn:incoming>
      <bpmn:outgoing>Flow_HandlePaymentFailure_to_PaymentFailed</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:endEvent id="EndEvent_PaymentFailed" name="Payment Failed">
      <bpmn:incoming>Flow_HandlePaymentFailure_to_PaymentFailed</bpmn:incoming>
      <bpmn:terminateEventDefinition />
    </bpmn:endEvent>
    <bpmn:sequenceFlow id="Flow_c453ctb" sourceRef="Event_phu49rm" targetRef="UserTask_HandlePaymentFailure" />
    <bpmn:sequenceFlow id="Flow_HandlePaymentFailure_to_PaymentFailed" sourceRef="UserTask_HandlePaymentFailure" targetRef="EndEvent_PaymentFailed" />
    <bpmn:scriptTask id="ScriptTask_LogTimeoutWarning" name="Log Timeout Warning" scriptFormat="groovy" camunda:resultVariable="logResult">
      <bpmn:outgoing>Flow_LogTimeoutWarning_to_TimeoutLogged</bpmn:outgoing>
      <bpmn:script>println "Charge taking long"</bpmn:script>
    </bpmn:scriptTask>
    <bpmn:endEvent id="EndEvent_TimeoutLogged" name="Timeout Logged">
      <bpmn:incoming>Flow_LogTimeoutWarning_to_TimeoutLogged</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:sequenceFlow id="Flow_LogTimeoutWarning_to_TimeoutLogged" sourceRef="ScriptTask_LogTimeoutWarning" targetRef="EndEvent_TimeoutLogged" />
    <bpmn:subProcess id="SubProcess_CancellationHandler" name="Cancellation Handler" triggeredByEvent="true">
      <bpmn:startEvent id="StartEvent_CancellationRequested" name="Cancellation Requested" isInterrupting="false">
        <bpmn:outgoing>Flow_CancellationRequested_to_ProcessCancellation</bpmn:outgoing>
        <bpmn:messageEventDefinition messageRef="Msg_CancellationRequest" />
      </bpmn:startEvent>
      <bpmn:userTask id="UserTask_ProcessCancellation" name="Process Cancellation">
        <bpmn:incoming>Flow_CancellationRequested_to_ProcessCancellation</bpmn:incoming>
        <bpmn:outgoing>Flow_ProcessCancellation_to_Cancelled</bpmn:outgoing>
      </bpmn:userTask>
      <bpmn:sequenceFlow id="Flow_CancellationRequested_to_ProcessCancellation" sourceRef="StartEvent_CancellationRequested" targetRef="UserTask_ProcessCancellation" />
      <bpmn:endEvent id="EndEvent_Cancelled" name="Cancelled">
        <bpmn:incoming>Flow_ProcessCancellation_to_Cancelled</bpmn:incoming>
      </bpmn:endEvent>
      <bpmn:sequenceFlow id="Flow_ProcessCancellation_to_Cancelled" sourceRef="UserTask_ProcessCancellation" targetRef="EndEvent_Cancelled" />
    </bpmn:subProcess>
  </bpmn:process>
