        </bpmn:timerEventDefinition>
      </bpmn:boundaryEvent>
    </bpmn:subProcess>
    <bpmn:sequenceFlow id="Flow_Start_to_ProcessPayment" sourceRef="StartEvent_Start" targetRef="SubProcess_ProcessPayment" />
    <bpmn:endEvent id="EndEvent_PaymentDone" name="Payment Done">
