  <bpmn:process id="Process_Payment_Processing" name="Payment Processing" isExecutable="true" camunda:historyTimeToLive="P180D">
    <bpmn:startEvent id="StartEvent_Start" name="Start">
      <bpmn:outgoing>Flow_Start_to_ProcessPayment</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:subProcess id="SubProcess_ProcessPayment" name="Process Payment">
      <bpmn:incoming>Flow_Start_to_ProcessPayment</bpmn:incoming>
      <bpmn:outgoing>Flow_ProcessPayment_to_PaymentDone</bpmn:outgoing>
      <bpmn:startEvent id="StartEvent_SubStart" name="Sub Start">
        <bpmn:outgoing>Flow_SubStart_to_ChargeCard</bpmn:outgoing>
      </bpmn:startEvent>
      <bpmn:serviceTask id="ServiceTask_ChargeCard" name="Charge Card">
        <bpmn:extensionElements>
          <camunda:executionListener class="com.example.PaymentAuditListener" event="end" />
        </bpmn:extensionElements>
        <bpmn:incoming>Flow_SubStart_to_ChargeCard</bpmn:incoming>
        <bpmn:outgoing>Flow_ChargeCard_to_SendReceipt</bpmn:outgoing>
      </bpmn:serviceTask>
      <bpmn:sequenceFlow id="Flow_SubStart_to_ChargeCard" sourceRef="StartEvent_SubStart" targetRef="ServiceTask_ChargeCard" />
      <bpmn:serviceTask id="ServiceTask_SendReceipt" name="Send Receipt">
        <bpmn:incoming>Flow_ChargeCard_to_SendReceipt</bpmn:incoming>
        <bpmn:outgoing>Flow_SendReceipt_to_SubEnd</bpmn:outgoing>
        <bpmn:multiInstanceLoopCharacteristics camunda:collection="${recipients}" camunda:elementVariable="recipient" />
      </bpmn:serviceTask>
      <bpmn:sequenceFlow id="Flow_ChargeCard_to_SendReceipt" sourceRef="ServiceTask_ChargeCard" targetRef="ServiceTask_SendReceipt" />
      <bpmn:endEvent id="EndEvent_SubEnd" name="Sub End">
        <bpmn:incoming>Flow_SendReceipt_to_SubEnd</bpmn:incoming>
      </bpmn:endEvent>
      <bpmn:sequenceFlow id="Flow_SendReceipt_to_SubEnd" sourceRef="ServiceTask_SendReceipt" targetRef="EndEvent_SubEnd" />
      <bpmn:boundaryEvent id="Event_8u5noze" attachedToRef="ServiceTask_ChargeCard">
        <bpmn:compensateEventDefinition />
      </bpmn:boundaryEvent>
      <bpmn:intermediateCatchEvent id="Event_gcvt6rv">
        <bpmn:timerEventDefinition id="TimerEventDefinition_0s0jskh">
          <bpmn:timeDuration xsi:type="bpmn:tFormalExpression">PT30S</bpmn:timeDuration>
        </bpmn:timerEventDefinition>
      </bpmn:intermediateCatchEvent>
      <bpmn:association id="Flow_nv9ummt" sourceRef="Event_8u5noze" targetRef="ServiceTask_RefundPayment" />
    </bpmn:subProcess>
    <bpmn:sequenceFlow id="Flow_Start_to_ProcessPayment" sourceRef="StartEvent_Start" targetRef="SubProcess_ProcessPayment" />
    <bpmn:endEvent id="EndEvent_PaymentDone" name="Payment Done">
      <bpmn:incoming>Flow_ProcessPayment_to_PaymentDone</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:sequenceFlow id="Flow_ProcessPayment_to_PaymentDone" sourceRef="SubProcess_ProcessPayment" targetRef="EndEvent_PaymentDone" />
    <bpmn:userTask id="UserTask_HandlePaymentFailure" name="Handle Payment Failure">
      <bpmn:incoming>Flow_b92gj1q</bpmn:incoming>
      <bpmn:outgoing>Flow_HandlePaymentFailure_to_PaymentFailed</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:endEvent id="EndEvent_PaymentFailed" name="Payment Failed">
      <bpmn:incoming>Flow_HandlePaymentFailure_to_PaymentFailed</bpmn:incoming>
      <bpmn:terminateEventDefinition />
    </bpmn:endEvent>
    <bpmn:sequenceFlow id="Flow_b92gj1q" sourceRef="Event_wnfese9" targetRef="UserTask_HandlePaymentFailure" />
    <bpmn:sequenceFlow id="Flow_HandlePaymentFailure_to_PaymentFailed" sourceRef="UserTask_HandlePaymentFailure" targetRef="EndEvent_PaymentFailed" />
    <bpmn:scriptTask id="ScriptTask_LogTimeoutWarning" name="Log Timeout Warning" scriptFormat="groovy" camunda:resultVariable="logResult">
      <bpmn:outgoing>Flow_LogTimeoutWarning_to_TimeoutLogged</bpmn:outgoing>
      <bpmn:script>println "Charge taking long"</bpmn:script>
    </bpmn:scriptTask>
    <bpmn:endEvent id="EndEvent_TimeoutLogged" name="Timeout Logged">
      <bpmn:incoming>Flow_LogTimeoutWarning_to_TimeoutLogged</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:sequenceFlow id="Flow_LogTimeoutWarning_to_TimeoutLogged" sourceRef="ScriptTask_LogTimeoutWarning" targetRef="EndEvent_TimeoutLogged" />
    <bpmn:subProcess id="SubProcess_CancellationHandler" name="Cancellation Handler" triggeredByEvent="true">
      <bpmn:startEvent id="StartEvent_CancellationRequested" name="Cancellation Requested" isInterrupting="false">
        <bpmn:outgoing>Flow_CancellationRequested_to_ProcessCancellation</bpmn:outgoing>
        <bpmn:messageEventDefinition messageRef="Msg_CancellationRequest" />
      </bpmn:startEvent>
      <bpmn:userTask id="UserTask_ProcessCancellation" name="Process Cancellation">
        <bpmn:incoming>Flow_CancellationRequested_to_ProcessCancellation</bpmn:incoming>
        <bpmn:outgoing>Flow_ProcessCancellation_to_Cancelled</bpmn:outgoing>
      </bpmn:userTask>
      <bpmn:sequenceFlow id="Flow_CancellationRequested_to_ProcessCancellation" sourceRef="StartEvent_CancellationRequested" targetRef="UserTask_ProcessCancellation" />
      <bpmn:endEvent id="EndEvent_Cancelled" name="Cancelled">
        <bpmn:incoming>Flow_ProcessCancellation_to_Cancelled</bpmn:incoming>
      </bpmn:endEvent>
      <bpmn:sequenceFlow id="Flow_ProcessCancellation_to_Cancelled" sourceRef="UserTask_ProcessCancellation" targetRef="EndEvent_Cancelled" />
    </bpmn:subProcess>
    <bpmn:intermediateCatchEvent id="Event_wnfese9">
      <bpmn:outgoing>Flow_b92gj1q</bpmn:outgoing>
      <bpmn:errorEventDefinition id="ErrorEventDefinition_1lr4gg8" errorRef="Error_PaymentFailed" />
    </bpmn:intermediateCatchEvent>
    <bpmn:serviceTask id="ServiceTask_RefundPayment" name="Refund Payment" isForCompensation="true" />
  </bpmn:process>
