      <bpmn:sequenceFlow id="Flow_CancellationRequested_to_ProcessCancellation" sourceRef="StartEvent_CancellationRequested" targetRef="UserTask_ProcessCancellation" />
      <bpmn:endEvent id="EndEvent_Cancelled" name="Cancelled">
