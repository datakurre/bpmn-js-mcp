<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" xmlns:di="http://www.omg.org/spec/DD/20100524/DI" xmlns:camunda="http://camunda.org/schema/1.0/bpmn" id="Definitions_1" targetNamespace="http://bpmn.io/schema/bpmn" exporter="bpmn-js-mcp" exporterVersion="1.0.0">
  <bpmn:process id="Process_ExclusiveGateway" name="Exclusive Gateway - Approval Flow" isExecutable="true" camunda:historyTimeToLive="P180D">
    <bpmn:startEvent id="Start" name="Request Submitted" camunda:initiator="submitter">
      <bpmn:outgoing>Flow1</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:userTask id="ReviewTask" name="Review Request" camunda:candidateGroups="reviewers">
      <bpmn:extensionElements>
        <camunda:formData>
          <camunda:formField id="approved" label="Approve?" type="boolean" defaultValue="false" />
          <camunda:formField id="reason" label="Reason" type="string">
            <camunda:validation>
              <camunda:constraint name="required" />
              <camunda:constraint name="minlength" config="10" />
            </camunda:validation>
          </camunda:formField>
        </camunda:formData>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow1</bpmn:incoming>
      <bpmn:outgoing>Flow2</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:exclusiveGateway id="GW_Split" name="Approved?" default="Flow_No">
      <bpmn:incoming>Flow2</bpmn:incoming>
      <bpmn:outgoing>Flow_Yes</bpmn:outgoing>
      <bpmn:outgoing>Flow_No</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:serviceTask id="ProcessApproval" name="Process Approval" camunda:expression="${approvalService.process(execution)}" camunda:resultVariable="result">
      <bpmn:incoming>Flow_Yes</bpmn:incoming>
      <bpmn:outgoing>Flow3</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:sendTask id="NotifyRejection" name="Notify Rejection" camunda:delegateExpression="${rejectionNotifier}">
      <bpmn:incoming>Flow_No</bpmn:incoming>
      <bpmn:outgoing>Flow4</bpmn:outgoing>
    </bpmn:sendTask>
    <bpmn:exclusiveGateway id="GW_Merge">
      <bpmn:incoming>Flow3</bpmn:incoming>
      <bpmn:incoming>Flow4</bpmn:incoming>
      <bpmn:outgoing>Flow5</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:endEvent id="End" name="Request Handled">
      <bpmn:incoming>Flow5</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:sequenceFlow id="Flow1" sourceRef="Start" targetRef="ReviewTask" />
    <bpmn:sequenceFlow id="Flow2" sourceRef="ReviewTask" targetRef="GW_Split" />
    <bpmn:sequenceFlow id="Flow_Yes" name="Yes" sourceRef="GW_Split" targetRef="ProcessApproval">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${approved == true}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_No" name="No" sourceRef="GW_Split" targetRef="NotifyRejection" />
    <bpmn:sequenceFlow id="Flow3" sourceRef="ProcessApproval" targetRef="GW_Merge" />
    <bpmn:sequenceFlow id="Flow4" sourceRef="NotifyRejection" targetRef="GW_Merge" />
    <bpmn:sequenceFlow id="Flow5" sourceRef="GW_Merge" targetRef="End" />
  </bpmn:process>
  <bpmndi:BPMNDiagram id="BPMNDiagram_Process_ExclusiveGateway">
    <bpmndi:BPMNPlane id="BPMNPlane_Process_ExclusiveGateway" bpmnElement="Process_ExclusiveGateway">
      <bpmndi:BPMNShape id="Start_di" bpmnElement="Start">
        <dc:Bounds x="192" y="122" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="185" y="158" width="50" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="ReviewTask_di" bpmnElement="ReviewTask">
        <dc:Bounds x="288" y="100" width="100" height="80" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="GW_Split_di" bpmnElement="GW_Split" isMarkerVisible="true">
        <dc:Bounds x="453" y="115" width="50" height="50" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="451" y="91" width="54" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="GW_Merge_di" bpmnElement="GW_Merge" isMarkerVisible="true">
        <dc:Bounds x="733" y="115" width="50" height="50" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="End_di" bpmnElement="End">
        <dc:Bounds x="838" y="122" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="814" y="158" width="85" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="ProcessApproval_di" bpmnElement="ProcessApproval">
        <dc:Bounds x="568" y="100" width="100" height="80" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="NotifyRejection_di" bpmnElement="NotifyRejection">
        <dc:Bounds x="568" y="210" width="100" height="80" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNEdge id="Flow1_di" bpmnElement="Flow1">
        <di:waypoint x="228" y="140" />
        <di:waypoint x="288" y="140" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow2_di" bpmnElement="Flow2">
        <di:waypoint x="388" y="140" />
        <di:waypoint x="453" y="140" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_Yes_di" bpmnElement="Flow_Yes">
        <di:waypoint x="503" y="140" />
        <di:waypoint x="568" y="140" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="514" y="121" width="19" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_No_di" bpmnElement="Flow_No">
        <di:waypoint x="478" y="165" />
        <di:waypoint x="478" y="250" />
        <di:waypoint x="568" y="250" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="472" y="184" width="15" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow3_di" bpmnElement="Flow3">
        <di:waypoint x="668" y="140" />
        <di:waypoint x="733" y="140" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow4_di" bpmnElement="Flow4">
        <di:waypoint x="668" y="250" />
        <di:waypoint x="758" y="250" />
        <di:waypoint x="758" y="165" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow5_di" bpmnElement="Flow5">
        <di:waypoint x="783" y="140" />
        <di:waypoint x="838" y="140" />
      </bpmndi:BPMNEdge>
    </bpmndi:BPMNPlane>
  </bpmndi:BPMNDiagram>
</bpmn:definitions>
