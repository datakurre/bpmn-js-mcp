<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" xmlns:di="http://www.omg.org/spec/DD/20100524/DI" xmlns:camunda="http://camunda.org/schema/1.0/bpmn" id="Definitions_1" targetNamespace="http://bpmn.io/schema/bpmn">
  <bpmn:message id="Message_Response" name="CustomerResponse" />
  <bpmn:process id="Process_EventBasedGateway" name="Event-Based Gateway - Wait for Response" isExecutable="true" camunda:historyTimeToLive="P180D">
    <bpmn:startEvent id="Start" name="Inquiry Sent">
      <bpmn:outgoing>Flow1</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:sendTask id="SendInquiry" name="Send Inquiry" camunda:delegateExpression="${inquirySender}">
      <bpmn:incoming>Flow1</bpmn:incoming>
      <bpmn:outgoing>Flow2</bpmn:outgoing>
    </bpmn:sendTask>
    <bpmn:eventBasedGateway id="EBG" name="Await Response">
      <bpmn:incoming>Flow2</bpmn:incoming>
      <bpmn:outgoing>Flow_Msg</bpmn:outgoing>
      <bpmn:outgoing>Flow_Timer</bpmn:outgoing>
    </bpmn:eventBasedGateway>
    <bpmn:intermediateCatchEvent id="MsgCatch" name="Response Received">
      <bpmn:incoming>Flow_Msg</bpmn:incoming>
      <bpmn:outgoing>Flow3</bpmn:outgoing>
      <bpmn:messageEventDefinition id="MsgDef1" messageRef="Message_Response" />
    </bpmn:intermediateCatchEvent>
    <bpmn:intermediateCatchEvent id="TimerCatch" name="Timeout (48h)">
      <bpmn:incoming>Flow_Timer</bpmn:incoming>
      <bpmn:outgoing>Flow4</bpmn:outgoing>
      <bpmn:timerEventDefinition id="TimerDef1">
        <bpmn:timeDuration>PT48H</bpmn:timeDuration>
      </bpmn:timerEventDefinition>
    </bpmn:intermediateCatchEvent>
    <bpmn:serviceTask id="ProcessResponse" name="Process Response" camunda:type="external" camunda:topic="process-response">
      <bpmn:incoming>Flow3</bpmn:incoming>
      <bpmn:outgoing>Flow5</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:userTask id="HandleTimeout" name="Handle Timeout" camunda:candidateGroups="support">
      <bpmn:incoming>Flow4</bpmn:incoming>
      <bpmn:outgoing>Flow6</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:endEvent id="EndOK" name="Response Processed">
      <bpmn:incoming>Flow5</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:endEvent id="EndTimeout" name="Timed Out">
      <bpmn:incoming>Flow6</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:sequenceFlow id="Flow1" sourceRef="Start" targetRef="SendInquiry" />
    <bpmn:sequenceFlow id="Flow2" sourceRef="SendInquiry" targetRef="EBG" />
    <bpmn:sequenceFlow id="Flow_Msg" sourceRef="EBG" targetRef="MsgCatch" />
    <bpmn:sequenceFlow id="Flow_Timer" sourceRef="EBG" targetRef="TimerCatch" />
    <bpmn:sequenceFlow id="Flow3" sourceRef="MsgCatch" targetRef="ProcessResponse" />
    <bpmn:sequenceFlow id="Flow4" sourceRef="TimerCatch" targetRef="HandleTimeout" />
    <bpmn:sequenceFlow id="Flow5" sourceRef="ProcessResponse" targetRef="EndOK" />
    <bpmn:sequenceFlow id="Flow6" sourceRef="HandleTimeout" targetRef="EndTimeout" />
  </bpmn:process>
